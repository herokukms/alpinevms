+        private void ExportVlmcsdToFile(string outputFile)
+        {
+            // Initialize LoadDatabase delegate
+            KmsLists.LoadDatabase = () =>
+            {
+                if (IsDatabaseLoaded)
+                {
+                    return;
+                }
+
+                try
+                {
+                    using FileStream stream = new FileStream(DatabaseFileName, FileMode.Open, FileAccess.Read, FileShare.Read);
+                    KmsLists.ReadDatabase(stream);
+                    IsDatabaseLoaded = true;
+                    DataBaseLoaded?.Invoke();
+                }
+                catch (Exception ex)
+                {
+                    if (!(ex is FileNotFoundException || ex.InnerException is FileNotFoundException))
+                    {
+                        Console.Error.WriteLine($"The KMS database \"{DatabaseFileName}\" could not be read:\n{ex.Message}\n\nUsing License Manager's built-in database");
+                    }
+                    using Stream stream = GetResourceStream(new Uri("pack://application:,,,/LmInternalDatabase.xml"))?.Stream;
+                    KmsLists.ReadDatabase(stream);
+                    IsDatabaseLoaded = true;
+                    DataBaseLoaded?.Invoke();
+                }
+            };
+
+            KmsLists.GetXsdValidationStream = () => GetResourceStream(new Uri("pack://application:,,,/KmsDataBase.xsd"))?.Stream;
+
+            // Load database
+            KmsLists.LoadDatabase();
+
+            // Generate Vlmcsd data
+            VlmcsdHeader vlmcsdHeader = default(VlmcsdHeader);
+            byte[] vlmcsdBytes;
+
+            using (MemoryStream stream = vlmcsdHeader.WriteData(true, true, true, false, false)) // Default options: include app, kms, sku, no description, no beta
+            {
+                vlmcsdBytes = stream.ToArray();
+            }
+
+            string text = "uint8_t DefaultKmsData[] =\n{\n";
+
+            for (int i = 0; i < vlmcsdBytes.Length; i += 16)
+            {
+                text += "\t/* " + $"{i:X4}" + " */ ";
+
+                for (int j = 0; j < 16; j++)
+                {
+                    if (i + j < vlmcsdBytes.Length)
+                    {
+                        text += $"0x{vlmcsdBytes[i + j]:X2}, ";
+                    }
+                    else
+                    {
+                        text += "      ";
+                    }
+                }
+
+                text += "  // ";
+
+                for (int j = 0; j < 16; j++)
+                {
+                    if (i + j >= vlmcsdBytes.Length)
+                    {
+                        continue;
+                    }
+
+                    byte character = vlmcsdBytes[i + j];
+                    text += (character < 0x20 || (j == 15 && character == 0x5c)) || (character > 0x7e && character < 0xa1) ? '.' : (char)vlmcsdBytes[i + j];
+                }
+
+                text += Environment.NewLine;
+            }
+
+            text += "};";
+
+            // Write to file
+            File.WriteAllText(outputFile, text);
+        }
+
+        private void ShowHelp()
+        {
+            string helpText = @"License Manager - KMS License Management Tool
+
+Usage:
+  LicenseManager.exe [options]
+
+Options:
+  --export-vlmcsd <filename>    Export KMS database to C source file for vlmcsd
+                                Example: LicenseManager.exe --export-vlmcsd kmsdata.c
+  
+  --help, /?                    Display this help message
+
+Description:
+  When run without arguments, launches the graphical user interface.
+  
+  The --export-vlmcsd option generates a C source file containing the KMS
+  database in vlmcsd format, which can be used to compile vlmcsd with
+  updated KMS data.
+
+Examples:
+  LicenseManager.exe
+    Launches the GUI application
+
+  LicenseManager.exe --export-vlmcsd output.c
+    Exports KMS data to output.c and exits
+
+  LicenseManager.exe --help
+    Displays this help message
+";
+            Console.WriteLine(helpText);
+        }
+
+        private static bool IsRunAsAdministrator()
+        {
+            WindowsIdentity identity = WindowsIdentity.GetCurrent();
+            WindowsPrincipal principal = new WindowsPrincipal(identity);
+            return principal.IsInRole(WindowsBuiltInRole.Administrator);
+        }
+
+        private static void RestartAsAdministrator()
+        {
+            ProcessStartInfo startInfo = new ProcessStartInfo
+            {
+                UseShellExecute = true,
+                WorkingDirectory = Environment.CurrentDirectory,
+                FileName = Assembly.GetExecutingAssembly().Location,
+                Verb = "runas"
+            };
+
+            try
+            {
+                Process.Start(startInfo);
+            }
+            catch
+            {
+                // User cancelled UAC prompt
+                MessageBox.Show(
+                    "Administrator privileges are required to run the License Manager GUI.",
+                    "Elevated Privileges Required",
+                    MessageBoxButton.OK,
+                    MessageBoxImage.Warning);
+            }
+        }
+
         private void Application_Startup(object sender, StartupEventArgs e)
         {
+            // Check for command line arguments
+            if (e.Args.Length >= 1 && (e.Args[0] == "--help" || e.Args[0] == "/?"))
+            {
+                ShowHelp();
+                Shutdown();
+                return;
+            }
+
+            if (e.Args.Length >= 2 && e.Args[0] == "--export-vlmcsd")
+            {
+                string outputFile = e.Args[1];
+                ExportVlmcsdToFile(outputFile);
+                Shutdown();
+                return;
+            }
+
+            // For GUI mode, check if we need administrator privileges
+            if (!IsRunAsAdministrator())
+            {
+                RestartAsAdministrator();
+                Shutdown();
+                return;
+            }
